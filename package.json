{
  "name": "example-mcp-server",
  "version": "1.0.0",
  "description": "A simple example MCP server with basic tools and resources",
  "type": "module",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "start:stdio": "tsc && node build/index.js",
    "start:http": "tsc && node build/index-http.js",
    "start:mock-oauth": "tsc && node build/mock-oauth-server.js",
    "dev:stdio": "pnpx @modelcontextprotocol/inspector --config src/config.json --server rick_n_morty_stdio",
    "dev:http": "pnpx @modelcontextprotocol/inspector --config src/config.json --server rick_n_morty_http",
    "dev:mock-oauth": "tsx src/mock-oauth-server.ts"
  },
  "keywords": [
    "mcp",
    "server",
    "example"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "cors": "2.8.5",
    "express": "5.1.0",
    "graphql": "16.12.0",
    "graphql-request": "7.3.4"
  },
  "devDependencies": {
    "@modelcontextprotocol/inspector": "0.17.2",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.5",
    "@types/node": "^22.0.0",
    "oauth2-mock-server": "8.2.0",
    "tsx": "4.20.6",
    "typescript": "^5.6.3"
  }
}
